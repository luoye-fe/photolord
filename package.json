{
  "name": "photolord",
  "version": "1.0.0",
  "description": "Photo management application based on web tech. Analyze the photos located on your local server, and provide various convenient ways to maintain the photos. Such as object recognition, face recognition, location mark, artificial label, then, you can use any keyword to search photo.",
  "keywords": [],
  "scripts": {
    "start": "npm run build && egg-scripts start --daemon --title=photolord --framework=@midwayjs/web",
    "stop": "egg-scripts stop --title=photolord",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "check": "luckyeye"
  },
  "dependencies": {
    "@midwayjs/decorator": "^2.9.1",
    "@midwayjs/web": "^2.9.1",
    "egg": "^2.29.3",
    "egg-scripts": "^2.13.0",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@midwayjs/cli": "^1.0.0",
    "@midwayjs/egg-ts-helper": "^1.0.1",
    "@midwayjs/luckyeye": "^1.0.0",
    "@types/jest": "^26.0.10",
    "@types/node": "14",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cross-env": "^6.0.0",
    "eslint": "^7.22.0",
    "husky": "^5.2.0",
    "jest": "^26.4.0",
    "lint-staged": "^10.5.4",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "ts-jest": "^26.2.0",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luoye-fe/photolord.git"
  },
  "author": "luoye <luoyefe@gmail.com>",
  "license": "MIT",
  "commitlint": {
    "extend": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --no-stash",
      "commit-msg": "commitlint -e"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint",
    "*.{less,scss}": "stylelint"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-luckyeye": {
    "packages": [
      "midway_v2"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
