{
  "name": "photolord",
  "version": "1.0.0",
  "description": "Photo management application based on web tech. Analyze the photos located on your local server, and provide various convenient ways to maintain the photos. Such as object recognition, face recognition, location mark, artificial label, then, you can use any keyword to search photo.",
  "keywords": [],
  "scripts": {
    "start": "npm run build && egg-scripts start --daemon --title=photolord --framework=@midwayjs/web",
    "stop": "egg-scripts stop --title=photolord",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts",
    "dev-ui": "cd web-ui && icejs start",
    "test": "midway-bin test --ts",
    "build": "bash ./scripts/build.sh",
    "check": "luckyeye"
  },
  "dependencies": {
    "@midwayjs/decorator": "^2.9.1",
    "@midwayjs/web": "^2.9.1",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "axios": "^0.21.1",
    "egg": "^2.29.3",
    "egg-scripts": "^2.13.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@midwayjs/cli": "^1.0.0",
    "@midwayjs/egg-ts-helper": "^1.0.1",
    "@midwayjs/luckyeye": "^1.0.0",
    "@types/jest": "^26.0.10",
    "@types/node": "14",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cross-env": "^6.0.0",
    "eslint": "^6.8.0",
    "husky": "^5.2.0",
    "jest": "^26.4.0",
    "lint-staged": "^10.5.4",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^21.0.0",
    "ts-jest": "^26.2.0",
    "typescript": "^4.2.3",
    "@iceworks/spec": "^1.0.0",
    "ice.js": "^1.0.0",
    "@ali/build-plugin-ice-def": "^0.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luoye-fe/photolord.git"
  },
  "author": "luoye <luoyefe@gmail.com>",
  "license": "MIT",
  "commitlint": {
    "extend": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --no-stash",
      "commit-msg": "commitlint -e"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint",
    "*.{less,scss}": "stylelint"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-luckyeye": {
    "packages": [
      "midway_v2"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
